{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "NasLandingPage": "Server=127.0.0.1;Uid=nlp;Pwd=password;Database=NasLandingPage;Allow User Variables=true;SslMode=none"
  },
  "AllowedHosts": "*",
  "NasLandingPage": {
    "IsLinux": false,
    "DataDir": "\\\\192.168.0.60\\appdata\\nas-landing-page\\data"
  },
  "Rn.Metrics": {
    "enabled": false,
    "application": "NasLandingPage",
    "template": "nas_landing_page/{measurement}",
    "environment": "development"
  },
  "Rn.Metrics.Rabbit": {
    "enabled": true,
    "username": "bob",
    "password": "password",
    "virtualHost": "/",
    "host": "192.168.0.60",
    "port": 5672,
    "exchange": "amq.topic",
    "routingKey": "metrics.nlp",
    "backOffTimeSec": 15,
    "coolDownTimeSec": 300,
    "coolDownThreshold": 3,
    "maxCoolDownRuns": 0
  },
  "Rn.DbCommon": {
    "enableMetrics": false,
    "logSqlCommands": true,
    "defaultConnection": "NasLandingPage"
  }
}
